<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle" >
    <gradient
        android:startColor="@color/green_bg"
        android:endColor="@color/green_bg"
        android:angle="90" />
    <padding
        android:left="3dp"
        android:top="3dp"
        android:right="0dp"
        android:bottom="3dp" />
    <corners
        android:topLeftRadius="3dp" android:topRightRadius="0dp" android:bottomLeftRadius="0dp" android:bottomRightRadius="3dp" />
    <stroke
        android:width="1dp"
        android:color="@color/green_border" />
</shape>
